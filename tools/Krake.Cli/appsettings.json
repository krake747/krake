{
    "CultureInfo": "<provide culture info en-US>",
    "Apps": {
        "Comdirect": {
            "RootDirectory": "<provide app directory here>",
            "PortfolioFile": "<provide file name pattern for portfolio data>"
        }
    },
    "Email": {
        "EmailAuth": {
            "UserName": "<provide username>",
            "Password": "<provide password>"
        },
        "DefaultSender": {
            "DisplayName": "Krake Report Team",
            "Address": "<provide email address>"
        },
        "IncomingEmailServer": {
            "Type": "Imap",
            "Host": "<provide imap host>",
            "Port": 993,
            "UseSsl": true
        },
        "OutgoingEmailServer": {
            "Type": "Smtp",
            "Host": "<provide smtp host>",
            "Port": 587,
            "UseSsl": false,
            "UseStartTls": true
        }
    },
    "EmailTemplate": {
        "DisplayName": "<provide display name>",
        "From": "<provide from email address>",
        "To": "<provide to email address>",
        "BodyFormat": "<provide email body format>"
    },
    "ConnectionStrings": {
        "KrakeDB": "Server=tcp:localhost;Initial Catalog=KrakeDB;Persist Security Info=False;User ID=sa;Password=Admin#123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;"
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": {
            "Name": "Console"
        }
    }
}